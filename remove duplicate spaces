import java.util.*;
import java.io.*;
public class Main
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		int n,count=0;
		n=s.length();
		for(int i=0;i<n;i++)
		{
		    System.out.print(s.charAt(i));
		    if(s.charAt(i)==' ')
		    if(i<n-1)
		    {
		    while(s.charAt(i+1)==' ' && i<n-1) 
		    i++;
		    }
		}
	}
}
